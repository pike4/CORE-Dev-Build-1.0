<CORE>
  <imports>
    <templates.xml></templates.xml>
    <prototypes.xml></prototypes.xml>
  </imports>
  <menuSystem>
    <name>main</name>
    <menuScreens>
      <menuScreen>
        <name>fourth</name>
        <controls>
          <GUI_Area>
            <data>
              <x>200</x>
              <y>40</y>
              <w>400</w>
              <h>400</h>
            </data>
            <controls>
              <dragArea>
                <data>
                  <xOffset>0</xOffset>
                  <yOffset>0</yOffset>
                </data>
              </dragArea>
              <checkbox>
                <data>
                  <zIndex>10</zIndex>
                  <xOffset>50</xOffset>
                  <yOffset>20</yOffset>
                  <w>100</w>
                  <h>100</h>
                </data>
                <components>
                  <mouseProcessor></mouseProcessor>
                  <menuDestination>
                    <destination>main</destination>
                  </menuDestination>
                  <variableElement>
                    <zIndex>2</zIndex>
                    <startButton>blankDefault</startButton>
                    <startButton>blankHeld</startButton>
                    <startButton>blankHover</startButton>
                    <startButton>buttonDefault</startButton>
                    <imageElement>
                      <name>buttonHover</name>
                      <image>Assets/Sprites/ButtonDefault.png</image>
                    </imageElement>
                  </variableElement>
                </components>
              </checkbox>
            </controls>

          </GUI_Area>
        </controls>
      </menuScreen>

