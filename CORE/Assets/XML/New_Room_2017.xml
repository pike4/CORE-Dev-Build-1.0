<CORE>
  <templates>
    <entity name="man">
      <sources>
        <x>xType, xLink, xVal</x>
        <image>img</image>
      </sources>
      <components>
        <imageElement>
          <data>
            <x value="100" link="parent"/>
            <y value="50" link="parent"/>
            <w>100</w>
            <h>100</h>
          </data>
          <image>?img</image>
          <zIndex>20</zIndex>
        </imageElement>
      </components>
      <data>
        <x type="?xType" value="?xVal"/>
        <y type="int" value="20"/>
        <w value ="36"/>
        <h value ="50"/>
      </data>
      <name>$manName</name>
    </entity>
  </templates>
  <environment>
    <name>first</name>
    <rooms>
      <room>
        <name>living room</name>
        <objects>
          <man>
            <x>int, self, 200</x>
            <image>Assets/Sprites/Happy_Cow_Man.png</image>
          </man>
          <entity>
            <data>
              <x value ="200"/>
              <y value ="200"/>
              <w value ="36"/>
              <h value ="50"/>
            </data>
            <components>
              <iVelocityControl></iVelocityControl>
              <imageElement>
                <data>
                  <x value="100" link="parent"/>
                  <y value="50" link="parent"/>
                  <w>100</w>
                  <h>100</h>
                </data>
                <image>Assets/Sprites/Happy_Cow_Man_Big.png</image>
               </imageElement>
            </components>
          </entity>
          <entity>
              <data>
                 <x value ="500"/>
                 <y value ="300"/>
                 <w value ="36"/>
                 <h value ="50"/>
              </data>
              <components>
                 <imageElement>
                    <data>
                       <x value="100" link="parent"/>
                       <y value="50" link="parent"/>
                       <w>100</w>
                       <h>100</h>
                    </data>
                    <image>Assets/Sprites/Happy_Cow_Man_Big.png</image>
                 </imageElement>
              </components>
           </entity>
          <StandardCollision>
            <format>
              <x type="int"/>
              <y type="int"/>
              <w type="int"/>
              <h type="int"/>
            </format>
          </StandardCollision>
        </objects>
      </room>
    </rooms>
  </environment>
</CORE>