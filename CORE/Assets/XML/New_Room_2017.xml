<CORE>
  <templates>
    <entity name="man">
      <sources>
        <x>xType, xLink, xVal</x>
        <image>img</image>
      </sources>
      <components>
        <imageElement>
          <data>
            <x value="100" link="parent"/>
            <y value="50" link="parent"/>
            <w>100</w>
            <h>100</h>
          </data>
          <image>?img</image>
          <zIndex>20</zIndex>
        </imageElement>
      </components>
      <data>
        <x type="?xType" value="?xVal"/>
        <y type="int" value="20"/>
        <w value ="36"/>
        <h value ="50"/>
      </data>
      <name>$manName</name>
    </entity>

    <entity name="dog">
      <sources>
        <x>xVal</x>
        <y>yVal</y>
      </sources>
      <data>
        <x type="int" link="self" value="?xVal"/>
        <y type="int" link="self" value="?yVal"/>
        <w value ="36"/>
        <h value ="50"/>
      </data>
      <components>
        <imageElement>
          <image>Assets/Sprites/Happy_Cow_Man_Big.png</image>
        </imageElement>
      </components>
    </entity>
    <entity name="cat">
      <sources>
        <x>xVal</x>
        <y>yVal</y>

      </sources>
      <data>
        <x type="int" link="self" value="?xVal"/>
        <y type="int" link="self" value="?yVal"/>
        <w value ="36"/>
        <h value ="50"/>
      </data>
      <components>
        <imageElement>
          <image>Assets/Sprites/Happy_Cow_Man_Big.png</image>
        </imageElement>
        <iVelocityControl/>
      </components>
    </entity>
  </templates>
  <environment>
    <name>first</name>
    <rooms>
      <living_room>
        <objects>
          <dog>
            <x>400</x>
            <y>400</y>
          </dog>
        </objects>
      </living_room>

      <garage>
        <objects>
          <dog>
            <x>100</x>
            <y>100</y>
          </dog>
          <dog>
            <x>300</x>
            <y>100</y>
          </dog>
        </objects>
      </garage>
    </rooms>
  </environment>
</CORE>