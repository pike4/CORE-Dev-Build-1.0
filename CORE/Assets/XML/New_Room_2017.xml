<CORE>
  <templates>
    <entity name="man">
      <sources>
        <x>xType, xLink, xVal</x>
        <image>img</image>
      </sources>
      <components>
        <imageElement>
          <data>
            <x value="100" link="parent"/>
            <y value="50" link="parent"/>
            <w>100</w>
            <h>100</h>
          </data>
          <image>?img</image>
          <zIndex>20</zIndex>
        </imageElement>
      </components>
      <data>
        <x type="?xType" value="?xVal"/>
        <y type="int" value="20"/>
        <w value ="36"/>
        <h value ="50"/>
      </data>
      <name>$manName</name>
    </entity>
  </templates>
  <environment>
    <name>first</name>
    <rooms>
      <room>
        <name>living room</name>
        <objects>
          <man>
            <x>int, self, 200</x>
            <image>Assets/Sprites/Happy_Cow_Man_Big.png</image>
          </man>

          <entity>
            <data>
              <x type="int" value="400"/>
              <y type="int" value="400"/>
            </data>
            <components>
              <imageElement>
                <data>
                  <x value="100" link="parent"/>
                  <y value="50" link="parent"/>
                  <w>100</w>
                  <h>100</h>
                </data>
                <zIndex>20</zIndex>
                <image>Assets/Sprites/Happy_cow_man_big.png</image>
              </imageElement>
              <iVelocityControl/>
            </components>
          </entity>
          
          <StandardCollision>
            <format>
              <x type="int"/>
              <y type="int"/>
              <w type="int"/>
              <h type="int"/>
            </format>
          </StandardCollision>
        </objects>
      </room>
    </rooms>
  </environment>
</CORE>